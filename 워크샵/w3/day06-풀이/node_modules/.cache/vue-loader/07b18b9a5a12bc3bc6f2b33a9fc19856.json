n            :key=\"index\"\r\n            :no=\"`${index + 1}`\"\r\n            :isbn=\"book.isbn\"\r\n            :title=\"book.title\"\r\n            :author=\"book.author\"\r\n            :price=\"book.price\"\r\n          />\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// http-common 사용\r\nimport http from \"@/util/http-common\";\r\nimport ListRow from \"@/components/book/include/ListRow.vue\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      books: [],\r\n    };\r\n  },\r\n  components: {\r\n    ListRow,\r\n  },\r\n  created() {\r\n    // axios의 get을 이용하여 비동기방식으로 서버와 통신.\r\n    http.get().then(({ data }) => {\r\n      this.books = data;\r\n    });\r\n  },\r\n  methods: {\r\n    movePage() {\r\n      // name이 book-create인 route로 변경.\r\n      this.$router.push({ name: \"book-create\" });\r\n    },\r\n    numberWithCommas(x) {\r\n      return x.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#book-list {\r\n  border-collapse: collapse;\r\n  width: 100%;\r\n}\r\n\r\n#book-list thead {\r\n  background-color: #ccc;\r\n  font-weight: bold;\r\n}\r\n\r\n#book-list td,\r\n#book-list th {\r\n  text-align: center;\r\n  border-bottom: 1px solid #ddd;\r\n  height: 50px;\r\n}\r\n\r\ntr:nth-child(even) {\r\n  background-color: #f2f2f2;\r\n}\r\n</style>\r\n"]}]}